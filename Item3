package com.nikolian;

public class Item3 {

    private String name;
    private double price;

    public Item3(String name, double price) {
        this.name = name;
        this.price = price;
    }

    public String getName() {
        return name;
    }
    public double getPrice() {
        return price;
    }

    public void addItem3(){
        System.out.println(this.name + " added");
    }

    public double finalPrice3(int count){
        this.price *= count;
        return this.price;
    }
}
