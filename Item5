package com.nikolian;

public class Item5 {

    private String name;
    private double price;

    public Item5(String name, double price) {
        this.name = name;
        this.price = price;
    }

    public String getName() {
        return name;
    }

    public double getPrice() {
        return price;
    }

    public void addItem5(){
        System.out.println(this.name + " added");
    }

    public double finalPrice5(int count){
        this.price *= count;
        return this.price;
    }
}
