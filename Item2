package com.nikolian;

public class Item2 {

    private String name;
    private double price;

    public Item2(String name, double price) {
        this.name = name;
        this.price = price;
    }

    public String getName() {
        return name;
    }
    public double getPrice() {
        return price;
    }

    public void addItem2(){
        System.out.println(this.name + " added");
    }
    public double finalPrice2(int count){
        this.price *= count;
        return this.price;
    }
}
