package com.nikolian;

public class Item6 {

    private String name;
    private double price;

    public Item6(String name, double price) {
        this.name = name;
        this.price = price;
    }

    public String getName() {
        return name;
    }

    public double getPrice() {
        return price;
    }

    public void addItem6(){
        System.out.println(this.name + " added");
    }

    public double finalPrice6(int count){
        this.price *= count;
        return this.price;
    }
}
